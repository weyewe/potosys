
<div class="span9 content">
  <%= create_guide("Draft Details", 
        "Office: <b>#{current_office.name}</b>"  + "<br />" + 
        "Draft: <b>Draft-#{@draft.number}</b>"  ).html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  <%# if flash[:notice] %>
    <div class="alert alert-success">
      <strong>Overall Feedback!</strong><br />
      <%= @draft.overall_feedback.html_safe %>
    </div>
  <%# end %>
  
  <% if flash[:error] %>
    <div class="alert alert-error">
      <a data-dismiss="alert" class="close">Ã—</a>
      <strong>Create Failure!</strong> Please fill in all fields.
    </div>
  <% end %>
  
   
  <div class="row-fluid">
    <div id="data_list">
      <h2>Total Tasks: <%= @tasks.count %></h2>
     <table cellpadding="0" cellspacing="0" border="0" 
           class="table table-striped table-bordered" id="example">
       <thead>
         <tr>
           <th>Date</th>
           <!-- <th style="width:50%;">Content</th>
                 <th>Discussions</th>
                 <th>Attachments</th>  -->
         </tr>
       </thead>




       <tbody>
         <% @tasks.each do |task| %>
           <tr>
             <td><%= task.created_at.to_date   %></td>
             <td><%= task.description.html_safe  %></td>
             <!-- <td><%= '--'  %></td>
                    <td><%= '--'  %></td>  -->
           </tr>
         <% end %>

       </tbody>
     </table>
    </div><!-- end of #data_list -->
  </div>

</div><!--/span-->













