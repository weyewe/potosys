
<div class="span9 content">
  <%= create_guide("Contact Reports", 
        "Office: <b>#{current_office.name}</b>"  + "<br />" + 
        "Employee: <b>#{current_user.email}</b>" + "<br />" + 
        "Client: <b>#{@client.name}</b>"  + "<br />" + 
        "Total Contact Report: <b>#{@total_contact_report}</b>"   ).html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  <div class="row-fluid">
    <div id="data_list">
      
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Employee</th>
            <th>Summary</th>
            <th>Description</th>
            <th>DateTime</th>
          </tr>
        </thead>
        
      
          
          
        <tbody>
          <% @contact_reports.each do |contact_report| %>
            <tr>
              <td><%= contact_report.user.email  %></td>
              <td><%= contact_report.summary   %></td>
              <td><%= contact_report.description  %></td>
              <td><%= contact_report.created_at  %></td>
            </tr>
          <% end %>

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>

</div><!--/span-->















